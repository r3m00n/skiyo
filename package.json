{
  "name": "skiyo",
  "version": "1.0.0",
  "module": "commonjs",
  "description": "Framework to find the best strategy for the game.",
  "main": "dist/src/index.js",
  "scripts": {
    "dev": "nodemon",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --collect-coverage",
    "build": "rimraf ./build && tsc",
    "start": "npm run build && node dist/src/index.js",
    "format": "npx prettier . --write"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run test && npm run prettier-format"
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.13",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "nodemon": "^3.1.2",
    "prettier": "3.2.5",
    "rimraf": "^5.0.7",
    "ts-jest": "^29.1.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  }
}
